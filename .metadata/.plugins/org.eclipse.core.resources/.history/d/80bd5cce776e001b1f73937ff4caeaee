package tt.ebay.stepDef;

import cucumber.api.PendingException;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;
import tt.ebay.pageAction.EbayCartActions;
import tt.ebay.pageAction.EbayHomepageActions;

public class EbayCartSteps {
	
	EbayHomepageActions EbayHomepageActionsobj = new EbayHomepageActions();
	EbayCartActions EbayCartActionsobj = new EbayCartActions();
	
	
	@When("^In home page click on cart icon$")
	public void in_home_page_click_on_cart_icon() throws Throwable {
		System.out.println("In home page click on cart icon");
		
		
	   
	    
	}

	@Then("^I land on shopping cart page$")
	public void i_land_on_shopping_cart_page() throws Throwable {
		System.out.println("I land on shopping cart page");
	   
	   
	}


}
